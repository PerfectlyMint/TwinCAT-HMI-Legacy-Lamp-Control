<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Legacy_Lamp</id>
    <version>1.0.1</version>
    <title>Legacy Lamp</title>
    <authors>Perfectly Mint</authors>
    <owners>Perfectly Mint</owners>
    <projectUrl>https://github.com/PerfectlyMint/TwinCAT-HMI-Legacy-Lamp-Control</projectUrl>
    <license type="expression">MIT</license>
    <icon>images/lamp.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An example code for TwinCAT HMI Framework extension. Produces the good ol' visualization lamp control.</description>
    <releaseNotes>Fixes on labels.</releaseNotes>
    <copyright>Copyright 2020</copyright>
    <tags>TwinCAT HMI</tags>
    <dependencies>
      <group targetFramework="native1.12-tchmi">
        <dependency id="Beckhoff.TwinCAT.HMI.Framework" version="12.742.5" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="**\*"
          target="runtimes\native1.12-tchmi\"
          exclude="**\*.nuspec;**\*.tpl.json;**\*.hmi*proj;**\~*;**\*.tmp;**\tmp*;**\*.user;**\tsconfig.json;**\*.log;**\*.bak;**\obj\**\*;packages.config;packages.xsd;.hmiframework\**\*;.git;.gitignore;.gitattributes" />
    <file src="images\logo.png" target="images\" />
    <file src="images\lamp.png" target="images\" />
  </files>
</package>
